{
  "name": "packsquash-gui",
  "version": "0.4.0",
  "description": "GUI for PackSquash",
  "repository": "https://github.com/ComunidadAylas/PackSquash",
  "private": true,
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "vite",
    "prebuild": "license-report --only=prod | jq -c \"del(.[].department, .[].relatedTo, .[].licensePeriod, .[].material, .[].remoteVersion, .[].definedVersion)\" > src/data/nodeDependencies.json",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/json-schema": "^7.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "license-report": "^6.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-solid": "^2.6.1"
  },
  "dependencies": {
    "@fluent/bundle": "^0.18.0",
    "@fluent/dom": "^0.9.0",
    "@fluent/langneg": "^0.7.0",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.0",
    "@tauri-apps/api": "^1.2.0",
    "phosphor-solid": "^1.1.5",
    "solid-confetti-explosion": "^1.1.6",
    "solid-headless": "^0.13.1",
    "solid-js": "^1.6.15",
    "solid-tippy": "^0.2.1",
    "solid-toast": "^0.4.0",
    "tailwindcss": "^3.2.7",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#971c3d86fb614871c49e27a30124454c66ca3a53",
    "tippy.js": "^6.3.7"
  }
}
