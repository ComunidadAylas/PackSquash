{
  "name": "packsquash-gui",
  "version": "0.4.0",
  "description": "GUI for PackSquash",
  "repository": "https://github.com/ComunidadAylas/PackSquash",
  "private": true,
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "vite",
    "prebuild": "license-report --only=prod | jq -c 'del(.[].department, .[].relatedTo, .[].licensePeriod, .[].material, .[].remoteVersion, .[].definedVersion)' > src/data/nodeDependencies.json",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@types/json-schema": "^7.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "license-report": "^6.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-solid": "^2.5.0"
  },
  "dependencies": {
    "@fluent/bundle": "^0.17.1",
    "@fluent/dom": "^0.8.1",
    "@fluent/langneg": "^0.6.2",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.7.0",
    "@tauri-apps/api": "^1.2.0",
    "phosphor-solid": "^1.1.5",
    "solid-confetti-explosion": "^1.1.6",
    "solid-headless": "^0.13.1",
    "solid-js": "^1.6.11",
    "solid-tippy": "^0.2.1",
    "solid-toast": "^0.4.0",
    "tailwindcss": "^3.2.7",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#4326f75446b8b9e0cb9904c65f258b81e23e544e",
    "tippy.js": "^6.3.7"
  }
}
